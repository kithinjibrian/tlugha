use std::io::{ print };
use std::fetch::{ post };

struct Resource {
    id: number;
    title: string;
    body: string;
    userId: number;
}

fun main(): Resource {
    let res = post("https://jsonplaceholder.typicode.com/posts", {
        title: "foo",
        body: "bar",
        userId: 1,
    }, {
        headers: {
            "Content-type": "application/json; charset=UTF-8",
        }
    });

    print("{}", res);

    return res;
}